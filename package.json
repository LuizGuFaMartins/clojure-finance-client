{
  "name": "clojure-finance-client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "tailwind:watch": "tailwindcss -i ./public/index.css -o ./resources/public/css/app.css --watch",
    "build:css": "tailwindcss -i ./public/index.css -o ./public/css/main.css",
    "build:js": "npx shadow-cljs release user-hub -A :final",
    "build:js:debug": "npx shadow-cljs release :user-hub -A :final --debug",
    "build": "npm-run-all build:js build:css",
    "watch:css": "npm run build:css -- --watch",
    "watch:js": "npx shadow-cljs watch :user-hub -A :dev",
    "dev": "npm-run-all --parallel watch:*"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "shadow-cljs": "^3.3.4",
    "tailwindcss": "^3.4.19"
  },
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs"
}
